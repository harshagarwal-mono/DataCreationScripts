# Database Configuration
# Host address of the database server
CDL_DATABASE_HOST=localhost

# Port number for the database connection
CDL_DATABASE_PORT=3306

# Name of the database to connect to
CDL_DATABASE_NAME=your_database_name

# Database user credentials
CDL_DATABASE_USERNAME=your_username
CDL_DATABASE_PASSWORD=your_password

# Batch Processing Configuration
# Number of events to process in a single batch
# Default: 1000
BATCH_SIZE=1000

# Number of parallel processing units for concurrent operations
# Higher values increase throughput but also increase system load
# Default: 4
PARALLEL_PROCESSING_UNITS=4
